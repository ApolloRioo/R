                if (match.startsWith("/") || match.startsWith("?")) {
                    return match.charAt(0)
                } else {
                    return ""
                }
            }
        });
        log(url)
        Apollo.pageAdd(page)
        const html = fetch(url, {
            headers: {
                'User-Agent': 'Mozilla/5.0 (Windows NT 10.0)'
            }
        })
        if (MY_PAGE == 1) {
            try {
                var title = "â€œâ€œâ€â€" + pdfh(html, ".img&&img&&alt").big().fontcolor("#FF1493") + "\n" + pdfh(html, ".model-list&&li:matches(é‡é‡)&&Text").fontcolor("#ef7251");
                var img = pdfh(html, ".img&&img&&src");
                var desc = "â€œâ€œâ€â€" + pdfh(html, ".model-list&&li:matches(æµ‹é‡)&&Text").big().fontcolor("#4169E1") + "\n" + pdfh(html, ".model-list&&li:matches(é«˜åº¦)&&Text").fontcolor("#00CED1").big();
