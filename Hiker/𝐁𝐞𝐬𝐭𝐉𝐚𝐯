        Apollo.d.push({
            title: 'â€œâ€œâ€â€' + "æˆ‘æ˜¯æœ‰åº•çº¿çš„".fontcolor("grey")
                .small(),
            url: "hiker://empty",
            col_type: "text_center_1",
            extra: {
                id: "ğğğ¬ğ­ğ‰ğšğ¯" + "footer",
                lineVisible: false
