                    });
                    putMyVar("ğğğ¬ğ­ğ‰ğšğ¯", id)
                    var ç« èŠ‚ = lists[getMyVar("ğğğ¬ğ­ğ‰ğšğ¯", '0')];
                    let cp = ç« èŠ‚.map((data, ssid) => {
