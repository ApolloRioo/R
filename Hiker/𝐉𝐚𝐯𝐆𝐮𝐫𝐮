    //ä¸€çº§åˆ—è¡¨
    videoType: (html) => {
        const list = pdfa(html, '#main&&.row')
        list.forEach(item => {
            javguru.d.push({
                title: pdfh(item, 'img&&alt'),
                url: $(pdfh(item, 'a&&href') + '#noHistory#')
                    .rule(() => {
                        const javguru = $.require('hiker://page/javguru?rule=ğ‰ğšğ¯ğ†ğ®ğ«ğ®')
                        javguru.videoParse(MY_URL)
