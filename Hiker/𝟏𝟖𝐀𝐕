    HostParse: (url, html) => {
        if (html.includes("åŒæ„(enter)")) {
            var cc = pdfh(html, ".scale_2&&input[type=submit]&&onclick").split("\"")[1]
            log("ç²å–cookieåœ°å€ï¼š" + cc)
