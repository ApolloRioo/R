            //java.lang.Thread.sleep(500);
            refreshPage()
        } else if (!html.includes("å‘å¸ƒé¡µåœ°å€") && !url.includes("æ²¡æœ‰ä¸‹ä¸€é¡µå“¦")) {
            showLoading('æ­£åœ¨è·å–åŸŸåï¼Œè¯·ç¨å€™');
            let host;
            var homeLink = "https://18av.mm-cg.com";
            if (request(homeLink).includes("è¯·æ¢æ–°ç½‘å€")) {
                var homePage = fetchCodeByWebView(homeLink, {
                    'blockRules': ["baidu.*.png", ".jpg", ".mp3", "m3u8", "mp4", ".flv", ".avi", ".3gp", ".mpeg", ".wmv", ".mov", ".rmvb", ".gif", ".jpg", ".jpeg", ".png", ".ico", ".svg"],
                    timeout: 5000
                });
                var links = pdfh(homePage, "b&&Html").split("<br><br> ");
                log(links)
                for (var i = 1; i < links.length - 1; i++) {
                    var releasePage = fetchCodeByWebView(links[i], {
                        'blockRules': ["baidu.*.png", ".jpg", ".mp3", "m3u8", "mp4", ".flv", ".avi", ".3gp", ".mpeg", ".wmv", ".mov", ".rmvb", ".gif", ".jpg", ".jpeg", ".png", ".ico", ".svg"],
                        timeout: 5000
                    });
                    if (releasePage.includes("1_8_A_V_ç™¼ä½ˆé ")) {
                        var hosts = pdfh(releasePage, ".list,5&&ul&&Html").match(/http.*?com/g);
                        //log(hosts)
                        log("å¤‡é€‰åŸŸå" + hosts.length + "ä¸ªï¼š\n" + hosts.join("\n"))
                        for (var j = 0; j < hosts.length; j++) {
                            var indexPage = request(hosts[j]);
                            if (indexPage.includes("åŒæ„(enter)") || indexPage.includes("18AVåœ¨ç·šHæˆäººå½±ç‰‡")) {
                                if (indexPage.includes("æœ¬åŸŸåå°†åœç”¨")) {
                                    host = pdfh(indexPage, ".di_img&&font,1&&a&&Text");
                                } else {
                                    host = hosts[j].replace("http", "https");
                                }
                                break;
                            }
                        }
                        break;
                    }
                }
            } else {
                host = homeLink;
                log("æŒ‚æ¢¯å­äº†ï¼Œç›´æ¥è®¿é—®")
            }
            log("è·å–åˆ°çš„åŸŸåï¼š\n" + host)
            setItem('link', host);
            hideLoading();
            //java.lang.Thread.sleep(200);
            refreshPage();
        }
    }

