            "title": "明星",
            "path": "",
            "type": "pornstar",
            "sub": [{
                "title": "模特",
                "path": "/pornstars"
            }, {
                "title": "已认证素人",
                "path": "/pornstars?performerType=amateur"
            }, {
                "title": "色情明星",
                "path": "/pornstars?performerType=pornstar"
            }, {
                "title": "男演员",
                "path": "/pornstars?gender=male"
            }]
