                if (match.endsWith('\d') || match.startsWith('&&')) {
                    return match.charAt(0) + 'earch/$1/' + page;
                } else if (match.endsWith('&')) {
                    return match.charAt(0) + `earch/${getMyVar('keyword', '')}/` + page + "&";
