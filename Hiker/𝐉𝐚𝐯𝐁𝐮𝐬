                                return 'hiker://empty'
                            })
                        }, {
                            title: 'å…¨éƒ¨å½±ç‰‡',
                            js: $.toString(() => {
                                setItem("mag", "existmag=all");
                                refreshPage();
                                return 'hiker://empty'
                            })
                        }]
                    }
                })
            } catch {}
            try {
                d.push({
                    title: "â€œâ€œâ€â€" + pdfh(html, ".alert-success&&Text").fontcolor("#666666"),
                    url: $().lazyRule(() => {
                        if (getItem("mag", "existmag=mag") != "existmag=mag") {
                            setItem("mag", "existmag=mag");
                            refreshPage();
                            return 'toast://åˆ‡æ¢åˆ°å·²æœ‰ç£åŠ›'
                        } else {
                            setItem("mag", "existmag=all");
                            refreshPage();
                            return 'toast://åˆ‡æ¢åˆ°å…¨éƒ¨å½±ç‰‡'
                        }
                    }),
                    col_type: "text_1",
                    extra: {
