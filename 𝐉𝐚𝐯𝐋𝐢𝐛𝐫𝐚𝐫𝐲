            var icon_s = 'https://hikerfans.com/weisyr/icon/';
            var list = pdfa(conts[i], 'body&&a');
            var avatar = icon_s + 'æ­£åº.svg';
            Apollo.d.push({
                title: line,
                desc: num,
                url: "hiker://empty",
                col_type: 'avatar',
                img: avatar,
            });
            for (var j = 0; j < list.length; j++) {
                var jm = pdfh(list[j], 'a&&Text');
                var url = pd(list[j], 'a&&href');
