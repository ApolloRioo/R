        function setTabs(tabs, taburl) {
            Apollo.d.push({
                col_type: "blank_block"
            })
            try {
                for (var i in tabs) {
                    var tabname = tabs[i];
                    Apollo.d.push({
                        title: (getMyVar("MY_TYPE") !== "ä¸»é¡µ" ?
                            (getMyVar("ysort", '0') == i ? "â€œâ€œâ€â€" + tabname.fontcolor(Apollo.getRangeColors()).bold() : tabname) :
                            (getMyVar(taburl, '0') == i ? "â€œâ€œâ€â€" + tabname.fontcolor(Apollo.getRangeColors()).bold() : tabname)),
                        col_type: 'scroll_button',
                        extra: {
                            'backgroundColor': (getMyVar("MY_TYPE") !== "ä¸»é¡µ" ?
                                (getMyVar("ysort", '0') == i ? Apollo.getRangeColors() : '') :
                                (getMyVar(taburl, '0') == i ? Apollo.getRangeColors() : ''))
                        },
                        url: tabu[i].replace(/.*(http.*)/, "$1") + $("#noLoading#").lazyRule((tabname, taburl, i) => {
                            if (getMyVar("MY_TYPE") !== "ä¸»é¡µ") {
                                putMyVar("ysort", i)
                                putMyVar("yurl", input)
                            } else {
                                putMyVar(taburl, i)
                                putMyVar("url", input)
                            }
                            clearMyVar("page");
                            clearMyVar("ypage");
                            refreshPage(false);
                            return 'hiker://empty'
                        }, tabname, taburl, i)
                    })
                }
            } catch {}
            Apollo.d.push({
                col_type: "blank_block"
            })
        }

        function setLists(lists, index) {
            var list = lists[index];

            function playLists() {
                try {
                    for (var j = 0; j < list.length; j++) {
                        var title = parseDomForHtml(list[j], æ ‡é¢˜);
                        var url = Apollo.url + parseDom(list[j], é“¾æ¥);
                        url = url.replace(/.*(http.*)/, "$1")
                        var col = æ ·å¼;
                        Apollo.d.push({
