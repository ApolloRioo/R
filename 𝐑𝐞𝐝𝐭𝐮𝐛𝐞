                                    }
                                    refreshPage();
                                    return 'toast://ä½ å·²ç»ç©¿è¶Šåˆ°äº†' + input + 'é¡µ'
                                } else {
                                    return "toast://è¯·è¾“å…¥æ­£ç¡®çš„é¡µç "
                                }
                            },
                            url, lastPage)
                },
                lastPage, url)
        };
        return longClick
