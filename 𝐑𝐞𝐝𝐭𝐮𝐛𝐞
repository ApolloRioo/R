    MovePageTo: function(url, currentPage, lastPage) {
        if (!currentPage) {
            currentPage = getMyVar("page") ? Number(getMyVar("page")) + Number(MY_PAGE) - 1 : MY_PAGE
        }
        if (!lastPage) {
            lastPage = "æœªçŸ¥"
