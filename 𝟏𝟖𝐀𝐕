                var title = pdfh(item, 'Text').replace(/(.*)\（.*\）/, "$1");
                Apollo.d.push({
                    title: getMyVar('ysort', '0') == index ? '‘‘’’<strong><font color="#ff1493">' + title + '</font></strong>' : title,
                    url: Apollo.url + url1 + pdfh(item, 'option&&value') + url3 + $('#noLoading#')
                        .lazyRule((index) => {
                        putMyVar('ysort', index);
                        putMyVar("yurl", input);
