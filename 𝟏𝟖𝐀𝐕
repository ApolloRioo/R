            }
        }
        if (MY_PAGE == 1 && !/genres\.php|search\.php/.test(url)) {
            Apollo.d.push({
                title: "🔍",
                url: $.toString(() => {
                    putMyVar('keyword', input);
                    var searchUrl = "https://www.javlibrary.com/cn/vl_searchbyid.php?keyword=" + input
                    try {
                        var url = JSON.parse(fetch(searchUrl, {
                            onlyHeaders: true
                        })).url;
                        if (url == searchUrl) {
                            putMyVar("url", searchUrl);
                            refreshPage(true);
