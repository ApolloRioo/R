                title: 'â€œâ€œâ€â€' + æ—¥æœŸ.fontcolor("#FF0000"),
                url: "hiker://search?rule=ğ‰ğšğ¯ğƒğ&s=" + num.replace(" [æ³„éœ²æœªå®¡æŸ¥]", ""),
                col_type: 'text_1',
                extra: {
                    lineVisiable: false
                }
            })
        }
