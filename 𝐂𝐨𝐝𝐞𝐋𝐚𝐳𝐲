                            var desc = pdfh(item, '.title&&Text')
                                .match(/[^\s]+/)[0] ? pdfh(item, '.title&&Text')
                                .match(/[^\s]+/)[0] : pdfh(item, '.title&&Text');
                            tktube.push({
                                title: desc,
                                url: pdfh(item, 'a&&href'),
                            })
                        })
                        if (tktube.length == 0) {
                            tktube = [{
