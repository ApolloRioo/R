                                var htm = fetch(miss[index].url);
                                var playUrl = "https://emb.missav.to/embedmv/hls/" + htm.match(/id=([^\'"&]+)/)[1] + ".m3u8"
                                return playUrl
                            }
                        }, miss)
                }, 识别码),
            col_type: 'icon_5',
            extra: {
                longClick: [{
                    title: '查看搜索',
                    js: $.toString((识别码) => {
                        return "https://missav.to/search/movie/" + 识别码
                    }, 识别码)
                }]
            }
