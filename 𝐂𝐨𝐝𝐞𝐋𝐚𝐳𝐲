                            },
                            timeout: 3223
                        })
                        if (html) {
                            var list = pdfa(html, 'body&&.video');
                            var MMTV = []
                            list.forEach(item => {
                                var title = pdfh(item, 'h3&&a&&Text')
                                    .match(/\[.*?\]/) ? pdfh(item, 'h3&&a&&Text')
                                    .replace(/(.*?)\s.*/, "$1")
                                    .replace("[中字]", "🇨🇳【中文字幕】")
                                    .replace("[無碼破解]", "🔥【无码破解】") : pdfh(item, 'h3&&a&&Text')
                                    .replace(/^((?:\[.*?\])?.*?)\s.*/, "🦋【默认视频】$1");
                                MMTV.push({
                                    title: title,
                                    url: pdfh(item, 'a&&href'),
                                })
                            })
                            if (MMTV.length == 0) {
                                MMTV = [{
                                    "title": "本平台尚未上传此番号视频",
                                    "url": "hiker://empty"
                                }]
                            }
                            var MMTVtype = MMTV.map(i => i.title)
                            return $(MMTVtype, 1)
                                .select((MMTV) => {
                                    if (input == '本平台尚未上传此番号视频') {
                                        return MMTV[0].url
                                    } else {
                                        let index = MMTV.findIndex(r => r.title == input);
                                        return MMTV[index].url + $('#noHistory#')
                                            .rule(() => {
                                                const Apollo = $.require('hiker://page/Apollo?rule=𝟕𝐌𝐌𝐓𝐕')
                                                Apollo.videoParse(MY_URL)
                                                setResult(Apollo.d)
                                            })
                                    }
                                }, MMTV)
                        } else {
                            return "confirm://网络不通，请检查通顺!.js:'hiker://empty' "
                        }
                    }
                }, 识别码),
            col_type: 'icon_5',
            extra: {
                longClick: [{
                    title: '查看搜索',
                    js: $.toString((识别码) => {
                        return "https://7mmtv.sx/zh/searchall_search/all/" + 识别码 + "/1.html";
                    }, 识别码)
                }]
            }
        });

        //𝐌𝐨𝐫𝐞
        d.push({
            title: '‘‘’’<b><middle><font color="#FF8C00">𝐌𝐨𝐫𝐞</font></middle>',
            pic_url: 'https://img.vinua.cn/images/IQkS8.png',
            url: $(["𝐀𝐯𝟎𝟏", "𝐖𝐚𝐧𝐭𝐚𝐯",
                    "𝐉𝐚𝐛𝐥𝐞", "𝐉𝐚𝐯𝐅𝐢𝐧𝐝𝐞𝐫"
                ], 3)
                .select((识别码) => {
                    if (input == "𝐖𝐚𝐧𝐭𝐚𝐯") {
                        var av = "https://www.wantav.com/videos/index/id/" + 识别码;
                        var html = fetch(av, {
                            //redirect: false,
                            timeout: 3223
                        })
                        if (html) {
                            var title = pdfh(html, "head&&title&&Text")
                            if (title.startsWith("wantav.com")) {
                                return "confirm://本平台尚未上传此番号视频.js:'hiker://empty' "
                            } else {
                                var playUrl = html.match(/hlsUrl.=."(.*?)"/)[1]
                                return playUrl
                            }
                        } else {
                            return "confirm://网络不通，请检查通顺!.js:'hiker://empty' "
                        }
                    }

                    if (input == "𝐀𝐯𝟎𝟏") {
                        var av = "https://www.av01.tv/" + 识别码;
                        var htmll = JSON.parse(fetch(av, {
                            onlyHeaders: true,
                            timeout: 3323
                        }))
                        // log(htmll)
                        if (htmll.statusCode != 0) {
                            var location = htmll.url
                            if (location.includes("error")) {
                                return "confirm://本平台尚未上传此番号视频.js:'hiker://empty' "
                            } else {
                                var html = fetch(location)
                                var src = "https://www.av01.tv" + pdfh(html, "script[referrerpolicy]&&src")
                                var load_video = fetch(src, {
                                    headers: {
                                        "Referer": location,
                                    }
                                })
                                var source = load_video.match(/const source.*?base64\,(.*?)\'/)[1]
                                var stream = base64Decode(source)
                                var video_quality = stream.match(/https?.*/g)
                                    .filter(v => !/\"/.test(v))
                                    .reverse()
                                var name_quality = stream.match(/\d+x\d+/g)
                                    .slice(0, video_quality.length)
                                    .reverse()
                                let playlist = {
                                    urls: video_quality,
                                    names: name_quality,
                                    headers: new Array(video_quality.length)
                                        .fill({
                                            "Origin": "https://www.av01.tv"
                                        })
                                };
                                return playlist;
                            }
                        } else {
                            return "confirm://网络不通，请检查通顺!.js:'hiker://empty' "
                        }
                    }

                    if (input == "𝐉𝐚𝐛𝐥𝐞") {
                        var av = "https://jable.tv/videos/" + 识别码.replace(/fc2.*?ppv.*?(\d+)/, "fc2ppv-$1") + "/";
                        try {
                            var html = fetch(av, {
                                timeout: 3223
                            })
                            if (html) {
                                var url = html.match(/https?:\/\/\S+?\.m3u8/)[0]
                                return url
                            } else {
                                return "confirm://网络不通，请检查通顺!.js:'hiker://empty' "
                            }
                        } catch {
                            return "confirm://本平台尚未上传此番号视频.js:'hiker://empty' "
                        }
                    }

                    if (input == "𝐉𝐚𝐯𝐅𝐢𝐧𝐝𝐞𝐫") {
                        var search = "https://javfinder.ai/search/movie/" + 识别码.replace(/fc2.*?ppv.*?(\d+)/, "fc2ppv $1");
                        var html = fetch(search, {
                            headers: {
                                'User-Agent': 'Mozilla/5.0 (Windows NT 10.0)'
                            },
                            redirect: false
                        })
                        var list = pdfa(html, '.videos-list&&article');
                        var JavFinder = []
                        list.forEach(item => {
                            var desc = pdfh(item, 'a&&title')
                                .match(/^\[(.*?)\]/) ? pdfh(item, 'a&&title')
                                .match(/^\[(.*?)\]/)[1] : "Original Video";
                            JavFinder.push({
                                title: desc,
                                url: "https://javfinder.ai" + pdfh(item, 'a&&href'),
                            })
                        })
                        if (JavFinder.length == 0) {
                            var redirect = JSON.parse(fetch(search, {
                                    onlyHeaders: true,
                                    timeout: 5000
                                }))
                                .url
                            if (/\.ai\/\d+/.test(redirect)) {
                                JavFinder = [{
                                    "title": pdfh(fetch(redirect), 'h1&&Text')
                                        .match(/^\[(.*?)\]/) ? pdfh(fetch(redirect), 'h1&&Text')
                                        .match(/^\[(.*?)\]/)[1] : "Original Video",
                                    "url": redirect
                                }]
                            } else {
                                JavFinder = [{
                                    "title": "本平台尚未上传此番号视频",
                                    "url": "hiker://empty"
                                }]
                            }
                        }
                        var JavFindertype = JavFinder.map(i => i.title)
                        return $(JavFindertype, 1)
                            .select((JavFinder) => {
                                if (input == '本平台尚未上传此番号视频') {
                                    return JavFinder[0].url
                                } else {
                                    let index = JavFinder.findIndex(r => r.title == input);
                                    try {
                                        var html = fetch(JavFinder[index].url, {
                                            headers: {
                                                'User-Agent': 'Mozilla/5.0 (Windows NT 10.0)'
                                            },
                                            timeout: 3000
                                        });
                                        var src = "https://javfinder.ai/stream/" + html.match(/iframe src.*?"(.*?)"/)[1].split("#")[1]
                                        var location = JSON.parse(fetch(src))
                                            .list[0].url
                                        var master = fetch(location);
                                        var script = master.match(/eval([\s\S]+?)<\/script/)[1];
                                        log(script)
                                        var uas = eval(script)
                                        var jwplayer = uas.match(/"(https?.*m3u8.*?)"/)[1]
                                        //log(jwplayer)  
                                        return jwplayer + ";{Referer@" + location + "}"
                                    } catch {
                                        return "toast://网络出错请重试"
                                    }
                                }
                            }, JavFinder)
                    }
                }, 识别码),
            col_type: 'icon_5',
            extra: {
                longClick: [{
                    title: '𝐀𝐯𝟎𝟏',
                    js: $.toString((识别码) => {
                        return "https://www.av01.tv/" + 识别码;
                    }, 识别码)
                }, {
                    title: '𝐖𝐚𝐧𝐭𝐚𝐯',
                    js: $.toString((识别码) => {
                        return "https://www.wantav.com/videos/index/id/" + 识别码;
                    }, 识别码)
                }, {
                    title: '𝐉𝐚𝐛𝐥𝐞',
                    js: $.toString((识别码) => {
                        return "https://jable.tv/videos/" + 识别码.replace(/fc2.*?ppv.*?(\d+)/, "fc2ppv-$1") + "/";
                    }, 识别码)
                }, {
                    title: '𝐉𝐚𝐯𝐅𝐢𝐧𝐝𝐞𝐫',
                    js: $.toString((识别码) => {
                        return "https://javfinder.ai/search/movie/" + 识别码;
                    }, 识别码)
                }]
            }
        });

        //磁力君.简
        d.push({
            title: '‘‘’’<b><middle><font color="#1E20800">𝐌𝐚𝐠𝐧𝐞𝐭</font></middle>',
            pic_url: 'https://img.vinua.cn/images/IQ85X.png',
            url: $('')
                .lazyRule((识别码) => {
                    if (request('hiker://home@磁力君.简') == 'null') {
                        confirm({
                            title: '依赖检测',
                            content: '检测到缺少磁力君.简\n请用力摁确定!',
                            confirm: () => {
                                return parsePaste("云6oooole/xxxxxx/osebx4awoxodbhro");
                            }
                        })
                        return "hiker://empty"
                    } else {
                        return "hiker://page/sou?rule=磁力君.简&p=fypage&searchTerms=" + 识别码
                    }
                }, 识别码),
            col_type: 'icon_5',
            extra: {
                longClick: [{
                    title: '谷歌搜索',
                    js: $.toString((识别码) => {
                        return "https://www.google.com/search?q=" + 识别码;
                    }, 识别码)
                }, {
                    title: 'Yandex搜索',
                    js: $.toString((识别码) => {
                        return "https://yandex.com/search/touch/?text=" + 识别码;
                    }, 识别码)
                }, {
                    title: '聚搜R',
                    js: $.toString((识别码) => {
                        return "hiker://search?s=" + 识别码 + "&group=①R&simple=false"
                    }, 识别码)
                }]
            }
        });

        d.push({
            col_type: "line_blank",
        })

    }