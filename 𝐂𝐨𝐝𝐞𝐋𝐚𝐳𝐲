                        return $(tktubetype, 1)
                            .select((tktube) => {
                                if (input == '本平台尚未上传此番号视频') {
                                    return tktube[0].url
                                } else {
                                    let index = tktube.findIndex(r => r.title == input);
                                    var html = fetch(tktube[index].url)
                                    var js = pdfh(html, "body&&.player-holder&&Html")
                                        .match(/var flashvar[^;]+/)[0]
                                    eval(js)
                                    if (flashvars.video_alt_url2) {
                                        let playlist = {
                                            urls: [flashvars.video_alt_url2, flashvars.video_alt_url, flashvars.video_url],
                                            names: [flashvars.video_alt_url2_text, flashvars.video_alt_url_text, flashvars.video_url_text],
                                        };
                                        return playlist;
                                        //+ "?rnd=" + flashvars.rnd
                                    } else if (flashvars.video_alt_url) {
                                        let playlist = {
                                            urls: [flashvars.video_alt_url, flashvars.video_url],
                                            names: [flashvars.video_alt_url_text, flashvars.video_url_text],
                                        };
                                        return playlist;
                                        //+ "?rnd=" + flashvars.rnd
                                    } else {
                                        let playlist = {
                                            urls: [flashvars.video_url],
                                            names: [flashvars.video_url_text],
                                        };
                                        return playlist;
                                        //+ "?rnd=" + flashvars.rnd
                                    }
                                }
                            }, tktube)
                    } else {
                        return "confirm://网络不通，请检查通顺!.js:'hiker://empty' "
                    }
                }, 识别码),
