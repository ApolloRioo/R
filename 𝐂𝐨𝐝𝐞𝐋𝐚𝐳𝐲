    function CodeLazy(识别码) {
        识别码 = 识别码.toLowerCase()
        d.push({
            col_type: "line_blank",
        })
        //ThissAV
        d.push({
            title: '‘‘’’<b><middle><font color="#FF0000">𝐓𝐡𝐢𝐬𝐀𝐕</font></middle>',
            pic_url: 'https://img.vinua.cn/images/IQlH7.png',
            url: $('')
                .lazyRule((识别码) => {
                    var search = "https://missav.com/cn/search/" + 识别码;
                    var html = fetch(search, {
                        headers: {
                            'User-Agent': 'Mozilla/5.0 (Windows NT 10.0)'
                        },
                        timeout: 3223
                    })
                    if (html) {
                        var list = pdfa(html, '.grid&&.relative:has(a[href])');
                        var miss = []
                        list.forEach(item => {
                            var desc = pdfh(item, 'a&&alt')
                                .replace("-uncensored-leak", "  🔥【无码破解】")
                                .replace("-chinese-subtitle", "  🇨🇳【中文字幕】")
                                .toUpperCase();
                            miss.push({
                                title: /\d$/.test(desc) ? desc + "  🦋【默认视频】" : desc,
                                url: pdfh(item, 'a&&href'),
                            })
                        })
                        if (miss.length == 0) {
                            miss = [{
                                "title": "本平台尚未上传此番号视频",
                                "url": "hiker://empty"
                            }]
                        }
                        var misstype = miss.map(i => i.title)
                        return $(misstype, 1)
                            .select((miss) => {
                                if (input == '本平台尚未上传此番号视频') {
                                    return miss[0].url
                                } else {
                                    try {
                                        let index = miss.findIndex(r => r.title == input);
                                        var html = fetch(miss[index].url, {
                                            headers: {
                                                'User-Agent': 'Mozilla/5.0 (Windows NT 10.0)'
                                            },
                                            timeout: 3000
                                        });
                                        const 标题 = pdfh(html, 'h1&&Text')
                                        const 图片 = pdfh(html, 'meta[property=og:image]&&content')
                                        eval(html.match(/eval.*?\n/)[0])
                                        // 获取画质列表，并取最高画质
                                        let group_quality = fetch(source, {
                                            headers: {
                                                "origin": "https://missav.com"
                                            }
                                        })
                                        hghest_quality = group_quality.match(/^(.*)\.m3u8$/gm)
                                            .map(v => source.replace("playlist.m3u8", v))
                                            .reverse()
                                        name_quality = group_quality.match(/RESOLUTION=.*$/gm)
                                            .map(n => n.replace("RESOLUTION=", ""))
                                            .reverse()
                                        let playlist = {
                                            urls: hghest_quality,
                                            names: name_quality,
                                            headers: new Array(hghest_quality.length)
                                                .fill({
                                                    origin: "https://missav.com/cn/"
                                                })
                                        };
                                        if (getVar("复制视频", "0") == "1") {
                                            return "copy://" + 标题 + "," + hghest_quality[0] + "," + 图片
                                        } else {
                                            return playlist;
                                        }
                                    } catch {
                                        return "toast://网络错误，请重试😓"
                                    }
                                }
                            }, miss)
                    } else {
                        return "confirm://网络不通，请检查通顺!.js:'hiker://empty' "
                    }
                }, 识别码),
            col_type: 'icon_5',
            extra: {
                longClick: [{
                    title: '查看搜索',
                    js: $.toString((识别码) => {
                        return "https://missav.com/cn/search/" + 识别码
                    }, 识别码)
                }, {
                    title: getVar("复制视频", "0") == "0" ? '播放视频⭕️' : "复制视频‼️",
                    js: $.toString(() => {
                        if (getVar("复制视频", "0") == "0") {
                            putVar("复制视频", "1")
                            refreshPage()
                        } else {
                            putVar("复制视频", "0")
                            refreshPage()
                        }
                        return "hiker://empty"
                    })
                }, {
                    title: '更新远程依赖',
                    js: $.toString(() => {
                        try {
                            var js = fetch("https://raw.githubusercontent.com/ApolloRioo/R/master/𝐂𝐨𝐝𝐞𝐋𝐚𝐳𝐲");
                            writeFile('hiker://files/rules/Apollo/𝐂𝐨𝐝𝐞𝐋𝐚𝐳𝐲.js', js);
                            refreshPage();
                            return "toast://远程依赖已更新"
                        } catch {
                            return "toast://请挂梯子"
                        }
                    })
                }]
            }
        });

        //123AV
        d.push({
            title: '‘‘’’<b><middle><font color="#1E90FF">𝟏𝟐𝟑𝐀𝐕</font></middle>',
            pic_url: 'https://img.vinua.cn/images/IQqwD.png',
            url: $('')
                .lazyRule((识别码) => {
                    var av = "https://123av.com/zh/v/" + 识别码 + "-uncensored-leaked";
                    var html = fetch(av, {
                        timeout: 3223
                    })
                    if (html) {
                        if (html.includes("请检查地址栏中的URL并重试")) {
                            var av = "https://123av.com/zh/v/" + 识别码;
                            var html = fetch(av, {
                                timeout: 3223
                            })
                        }
                        if (html.includes("请检查地址栏中的URL并重试")) {
                            return "confirm://本平台尚未上传此番号视频.js:'hiker://empty' "
                        }
                        var id = pdfh(html, "#page-video&&v-scope")
                            .match(/id:.(\d+)/)[1]
                        var video = "https://123av.com/zh/ajax/v/" + id + "/videos"
                        var data = JSON.parse(fetch(video, {
                            headers: {
                                "Referer": av
                            }
                        }))
                        var javplayer = data.data.watch[0].url
                        var playUrl = pdfh(fetch(javplayer), "#player&&v-scope")
                            .match(/https?.*?\.m3u8/)[0]
                        return playUrl.replace(/\\\//g, '/') + ";{Referer@" + javplayer + "}"
                    } else {
                        return "confirm://网络不通，请检查通顺!.js:'hiker://empty' "
                    }
                }, 识别码),
            col_type: 'icon_5',
            extra: {
                longClick: [{
                    title: '查看网址',
                    js: $.toString((识别码) => {
                        return "https://123av.com/zh/v/" + 识别码;
                    }, 识别码)
                }]
            }
        });

        //haokan
        d.push({
            title: '‘‘’’<b><middle><font color="#8A2BE2">𝐇𝐚𝐨𝐤𝐚𝐧</font></middle>',
            pic_url: 'https://img.vinua.cn/images/IQBFU.png',
            url: $('')
                .lazyRule((识别码) => {
                    var av = "http://sousou6.com/vod/search.html?wd=" + 识别码;
                    var html = fetch(av, {
                        timeout: 3323
                    })
                    if (html) {
                        var list = pdfa(html, ".post-list&&.video-item")
                        if (list.length < 1) {
                            return "confirm://本平台尚未上传此番号视频.js:'hiker://empty' "
                        } else {
                            var Alist = []
                            list.forEach(item => {
                                var title = pdfh(item, 'img&&alt');
                                Alist.push({
                                    title: title,
                                    url: "http://sousou6.com" + pdfh(item, 'a&&href'),
                                })
                            })
                            var Atitle = Alist.map(i => i.title)
                            return $(Atitle, 1)
                                .select((Alist) => {
                                    let index = Alist.findIndex(r => r.title == input);
                                    var html = fetch(Alist[index].url)
                                    var url = JSON.parse(html.match(/r player_.*?=(.*?)</)[1]).url
                                    return unescape(url)
                                }, Alist)
                        }
                    } else {
                        return "confirm://网络不通，请检查通顺!.js:'hiker://empty' "
                    }
                }, 识别码),
            col_type: 'icon_5',
            extra: {
                longClick: [{
                    title: '查看网址',
                    js: $.toString((识别码) => {
                        return "http://sousou6.com/vod/search.html?wd=" + 识别码;
                    }, 识别码)
                }]
            }
