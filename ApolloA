function CodeLazy(识别码) {
//ThissAV
d.push({
    title: '‘‘’’<b><middle><font color="#FF0000">𝐓𝐡𝐢𝐬𝐀𝐕</font></middle>',
    pic_url: 'https://cdn4.iconfinder.com/data/icons/free-large-boss-icon-set/128/Devil.png',
    url: $('').lazyRule((识别码) => {
         var search = "https://missav.com/cn/search/" + 识别码;
         var html = fetch(search,{headers:{'User-Agent': 'Mozilla/5.0 (Windows NT 10.0)'}})
         var list = pdfa(html, '.grid&&.relative:has(a[href])');        
         var miss = []
         list.forEach(item => {
              var desc = pdfh(item, '.absolute.left-1&&Text');
              miss.push({
                 title: desc == "" ? "默认视频" : desc,        
                 url: pdfh(item, 'a&&href'),     
             })
         })                 
         if (miss.length==0){
         miss = [{"title":"本平台尚未上传此番号视频","url":"hiker://empty"}]
         }
         var misstype = miss.map(i=>i.title)
    return $(misstype,1).select((miss) => {
                if (input == '本平台尚未上传此番号视频'){
                    // return miss[0].url
}else {
        let index = miss.findIndex(r=>r.title==input);
                      try{
                var html = fetch(miss[index].url, {
                    headers: {
                        'User-Agent': 'Mozilla/5.0 (Windows NT 10.0)'
                           },timeout:5000
                           });
                 eval(html.match(/eval.*?\n/)[0])
                    // 获取画质列表，并去最高画质
                    let group_quality = fetch(source,{headers:{"origin":"https://missav.com"}})
                    hghest_quality = group_quality.match(/^(.*)\.m3u8$/gm).map(v=>source.replace("playlist.m3u8", v)).reverse()
                   name_quality = group_quality.match(/RESOLUTION=.*$/gm).map(n=>n.replace("RESOLUTION=", "")).reverse()
        copy(hghest_quality[0])
        let playlist={
            urls: hghest_quality,
            names: name_quality,
            headers: new Array(hghest_quality.length).fill({origin:"https://missav.com/cn/"})
        };
        return playlist;                    
                 }catch{return "toast://挂梯 or 没有此影片"}
}
                  },miss)
                      },识别码),
    col_type: 'icon_5',
    extra: { longClick: [{title: '查看搜索', js: $.toString((识别码)=>{ return "https://missav.com/cn/search/" + 识别码},识别码)}] }
});

//MISSAV
d.push({
    title: '‘‘’’<b><middle><font color="#BEDA8F">𝐌𝐢𝐬𝐬𝐀𝐕</font></middle>',
    pic_url: 'https://cdn1.iconfinder.com/data/icons/fruits-n-vegetables-colored-gradient/128/cauliflower-128.png',
    url: $('').lazyRule((识别码) => {
       try{
             var av = "https://missav.to/search/movie/" + 识别码;  
             var urls = [];    
             var list = pdfa(fetch(av,{timeout:5000}),".videos-list&&article");
            for(var j in list){
              urls.push({
                       url: pdfh(list[j],"a&&href")
                        })
            }
            var plays = bf(urls).map(h=>"https://emb.missav.to/embedmv/hls/" + h.match(/id=([^\"&]+)/)[1] + ".m3u8")   
             return {urls:plays}
       }catch{
         try{
             var av = "https://missav.to/search/movie/" + 识别码.replace(/\-|\_/,"%20");  
             var urls = [];    
             var list = pdfa(fetch(av,{timeout:5000}),".videos-list&&article");
            for(var j in list){
              urls.push({
                       url: pdfh(list[j],"a&&href")
                        })
            }
            var plays = bf(urls).map(h=>"https://emb.missav.to/embedmv/hls/" + h.match(/id=([^\"&]+)/)[1] + ".m3u8")   
             return {urls:plays}
          }catch{return "toast://没有此影片"}
       }   
                  },识别码),
    col_type: 'icon_5',
    extra: { longClick: [{title: '查看网址', js: $.toString((识别码)=>{ return "https://missav.to/search/movie/" + 识别码},识别码)}] }
});

//Jable
d.push({
    title: '‘‘’’<b><middle><font color="#FF8C00">𝐉𝐚𝐛𝐥𝐞</font></middle>',
    pic_url: 'https://cdn3.iconfinder.com/data/icons/cat-power-premium/120/cat_foodlove-128.png',
    url: $('').lazyRule((识别码) => {
                 var av = "https://jable.tv/videos/" + 识别码 + "/"; 
//log(av)
                // request("https://jable.tv/")
              try{
                 var html = fetch(av,{timeout:5000})
                 var url = html.match(/https?:\/\/\S+?\.m3u8/)[0]
                 return url
                 }catch{return "toast://没有挂梯子或没有此影片"}
                  },识别码),
    col_type: 'icon_5',
    extra: { longClick: [{title: '查看网址', js: $.toString((识别码)=>{ return "https://jable.tv/videos/" + 识别码 + "/";},识别码)}] }
});

//njav
d.push({
    title: '‘‘’’<b><middle><font color="#2091C9">𝐧𝐉𝐚𝐯.𝐭𝐯</font></middle>',
    pic_url: 'https://cdn4.iconfinder.com/data/icons/iconza-poker-playing-cards/64/chips-blue-128.png',
    url: $('').lazyRule((识别码) => {
                 var av = "https://njav.tv/zh/v/" + 识别码; 
//log(av)
                 var html = fetch(av,{timeout:5000})
                 if(!html || html.includes("请检查地址栏中的网址")){         
                 return "toast://挂梯或没有此影片"
                   }else{
                 return "video://" + av;
                    }
               },识别码),
    col_type: 'icon_5',
    extra: {
                videoExcludeRules: ['240'],
                blockRules: ['*.doppiocdn.*'],
                blockRules: ['dmm.co.jp'],
                //videoRules: ['*m3u8*'],
                cacheM3u8: true,
               longClick: [{title: '查看网址', js: $.toString((识别码)=>{ return "https://njav.tv/zh/v/" + 识别码;},识别码)}]
                        }
});

//av01
d.push({
    title: '‘‘’’<b><middle><font color="#8A2BE2">𝐀𝐕𝟎𝟏</font></middle>',
    pic_url: 'https://cdn3.iconfinder.com/data/icons/butterfly-19/500/nim834_7_purple_butterfly_flower_tattoo_floral_retro-128.png',
    url: $('').lazyRule((识别码) => {
                 var av = "https://www.av01.tv/" + 识别码; 
//log(av)
                 var html = fetch(av,{timeout:5000})
                 var code = pdfh(html,'#plyr_video&&poster')
//log(code)
                 if(!code){         
                 return "toast://挂梯或没有此影片"
                   }else{
                 return "video://" + av;
                    }
               },识别码),
    col_type: 'icon_5',
    extra: {
                blockRules: ['.doppiocdn.','dmm.co.jp'],
                videoRules: ['index45va-v3-a1.m3u8','index45va-v2-a1.m3u8'],
                videoExcludeRules: ['doppiocdn','index45va-v1-a1.m3u8'],
                cacheM3u8: true,
               longClick: [{title: '查看网址', js: $.toString((识别码)=>{ return "https://www.av01.tv/" + 识别码;},识别码)}]
                        }
});


//ThissAV破解
d.push({
    title: '‘‘’’<b><middle><font color="#63A233">𝐂𝐫𝐚𝐜𝐤𝐞𝐝</font></middle>',
    pic_url: 'https://img.vinua.cn/images/zHhaL.png',
    url: $('').lazyRule((识别码) => {
                 var av = "https://missav.com/cn/" + 识别码 + "-uncensored-leak"; 
//log(av)
                 var bv = JSON.parse(fetch(av, {onlyHeaders:true})).url
                if (!bv.includes("-uncensored-leak") ){          
            return "toast://挂梯 or 此影片没有无码破解"
}else{
              try{
                 var html = fetch(av, {                    headers: {
                        'User-Agent': 'Mozilla/5.0 (Windows NT 10.0)'
                           },
             });
                 eval(html.match(/eval.*?\n/)[0])
                    // 获取画质列表，并去最高画质
                    let group_quality = fetch(source,{headers:{"origin":"https://missav.com"}})
                    hghest_quality = group_quality.match(/^(.*)\.m3u8$/gm).map(v=>source.replace("playlist.m3u8", v)).reverse()
                   name_quality = group_quality.match(/RESOLUTION=.*$/gm).map(n=>n.replace("RESOLUTION=", "")).reverse()
        copy(hghest_quality[0])
        let playlist={
            urls: hghest_quality,
            names: name_quality,
            headers: new Array(hghest_quality.length).fill({Referer:"https://missav.com/cn/"})
        };
        return playlist;                   
                 }catch{return "toast://挂梯 or 没有此影片"}
}
                  },识别码),
    col_type: 'icon_5',
    extra: { 
         longClick: [{title: '查看网址', js: $.toString((识别码)=>{ return "https://missav.com/cn/" + 识别码 + "-uncensored-leak";},识别码)}] }
});

