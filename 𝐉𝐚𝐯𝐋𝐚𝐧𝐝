                    col_type: 'scroll_button',
                    extra: {
                        pageTitle: makers
                    }
                })
            }
        } catch {}
        Apollo.d.push({
            col_type: "blank_block"
        })
        //ç³»åˆ—
        try {
            var makers = pdfh(html, 'body&&.k-right&&.videotextlist&&.series&&a&&Text');
